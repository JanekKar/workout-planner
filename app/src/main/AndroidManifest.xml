<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.workoutplanner">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.WorkoutPlanner">
        <activity android:name=".PastTrainingsActivity"></activity>
        <activity android:name=".TrainingActivity" />
        <activity android:name=".NewExerciseActivity" />
        <activity android:name=".ExerciseListActivity" />
        <activity android:name=".WorkoutActivity" />
        <activity android:name=".NewExerciseSetActivity" />
        <activity android:name=".NewWorkoutActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>